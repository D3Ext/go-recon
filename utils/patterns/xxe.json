{
  "description": "look for XML errors to find XXE vulnerabilities",
  "flags": "-HanriE",
  "patterns": [
    "simplexml_load_string",
    "parser error",
    "An error occured!",
    "xmlParseEntityDecl",
    "xmlParseInternalSubset",
    "DOCTYPE improperly terminated",
    "Start tag expected",
    "No declaration for attribute",
    "No declaration for element",
    "failed to load external entity",
    "Invalid URI: file:///",
    "Malformed declaration expecting version",
    "Unicode strings with encoding",
    "must be well-formed",
    "Content is not allowed in prolog",
    "org.xml.sax",
    "SAXParseException",
    "com.sun.org.apache.xerces",
    "ParseError",
    "nokogiri",
    "REXML",
    "XML syntax error on line",
    "Error unmarshaling XML",
    "conflicts with field",
    "illegal character code",
    "XML Parsing Error",
    "SyntaxError",
    "no root element",
    "not well-formed"
  ]
}

